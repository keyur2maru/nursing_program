{"ast":null,"code":"import confetti from 'canvas-confetti';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      approvedCandidates: [],\n      notApprovedCandidates: [],\n      searchQuery: ''\n    };\n  },\n  computed: {\n    filteredApprovedCandidates() {\n      return this.approvedCandidates.filter(candidate => candidate.name.toLowerCase().includes(this.searchQuery.toLowerCase()));\n    }\n    // Add a computed property for notApprovedCandidates if needed\n  },\n\n  methods: {\n    launchConfetti() {\n      const canvas = document.getElementById('confetti-canvas');\n      const myConfetti = confetti.create(canvas, {\n        resize: true\n      });\n      myConfetti({\n        particleCount: 150,\n        spread: 70,\n        origin: {\n          y: 0.6\n        }\n      });\n    }\n  },\n  mounted() {\n    // Fetch the candidate data from the JSON files when the component is created\n    import('./selected.json').then(module => {\n      this.approvedCandidates = module.selectedCandidates;\n      this.launchConfetti();\n    });\n    import('./rejected.json').then(module => {\n      this.notApprovedCandidates = module.rejectedCandidates;\n    });\n  }\n};","map":{"version":3,"names":["confetti","name","data","approvedCandidates","notApprovedCandidates","searchQuery","computed","filteredApprovedCandidates","filter","candidate","toLowerCase","includes","methods","launchConfetti","canvas","document","getElementById","myConfetti","create","resize","particleCount","spread","origin","y","mounted","then","module","selectedCandidates","rejectedCandidates"],"sources":["/Users/keyurmaru/Binary_Tree/Binary_Tree/nursing-college-app/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"min-h-screen bg-gray-100 py-10\">\n    <div class=\"container mx-auto\">\n      <input\n        v-model=\"searchQuery\"\n        type=\"text\"\n        placeholder=\"Search by name...\"\n        class=\"mb-4 p-2 border rounded\"\n      />\n      <div v-for=\"candidate in filteredApprovedCandidates\" :key=\"candidate.name\" class=\"p-4 bg-white shadow rounded mb-3\">\n    <h3 class=\"text-xl font-bold\">{{ candidate.name }}</h3>\n    <p>Total Score: {{ candidate.totalScore }}</p>\n    <table class=\"min-w-full table-auto border-collapse border border-gray-200 mt-4\">\n      <thead class=\"bg-gray-100\">\n        <tr>\n          <th class=\"border border-gray-300 p-2\">Student Name</th>\n          <th class=\"border border-gray-300 p-2\">TEAS Score >= 62?</th>\n          <th class=\"border border-gray-300 p-2\">Non-Science GPA >= 2.5?</th>\n          <th class=\"border border-gray-300 p-2\">Science GPA >= 3.0?</th>\n          <th class=\"border border-gray-300 p-2\">Has all Pre-Reqs?</th>\n          <th class=\"border border-gray-300 p-2\">Has AA/AS?</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr class=\"hover:bg-gray-50\">\n          <td class=\"border border-gray-300 p-2\">{{ candidate.name }}</td>\n          <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['TEAS Score >= 62?'] }}</td>\n          <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Non-Science GPA >= 2.5?'] }}</td>\n          <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Science GPA >= 3.0?'] }}</td>\n          <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Has all Pre-Reqs?'] }}</td>\n          <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Has AA/AS?'] }}</td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n  </div>  \n  </div>\n</template>\n\n<script>\nimport confetti from 'canvas-confetti';\n\nexport default {\n  name: 'App',\n  data() {\n    return {\n      approvedCandidates: [],\n      notApprovedCandidates: [],\n      searchQuery: '',\n    };\n  },\n  computed: {\n    filteredApprovedCandidates() {\n      return this.approvedCandidates.filter(candidate =>\n        candidate.name.toLowerCase().includes(this.searchQuery.toLowerCase())\n      );\n    },\n    // Add a computed property for notApprovedCandidates if needed\n  },\n  methods: {\n    launchConfetti() {\n      const canvas = document.getElementById('confetti-canvas');\n      const myConfetti = confetti.create(canvas, { resize: true });\n      myConfetti({\n        particleCount: 150,\n        spread: 70,\n        origin: { y: 0.6 },\n      });\n    },\n  },\n  mounted() {\n    // Fetch the candidate data from the JSON files when the component is created\n    import('./selected.json').then((module) => {\n      this.approvedCandidates = module.selectedCandidates;\n      this.launchConfetti();\n    });\n    import('./rejected.json').then((module) => {\n      this.notApprovedCandidates = module.rejectedCandidates;\n    });\n  },\n};\n</script>\n\n<style>\n#confetti-canvas {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  pointer-events: none;\n  z-index: 9999;\n}</style>\n"],"mappings":"AAwCA,OAAOA,QAAO,MAAO,iBAAiB;AAEtC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,kBAAkB,EAAE,EAAE;MACtBC,qBAAqB,EAAE,EAAE;MACzBC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,0BAA0BA,CAAA,EAAG;MAC3B,OAAO,IAAI,CAACJ,kBAAkB,CAACK,MAAM,CAACC,SAAQ,IAC5CA,SAAS,CAACR,IAAI,CAACS,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACN,WAAW,CAACK,WAAW,CAAC,CAAC,CACtE,CAAC;IACH;IACA;EACF,CAAC;;EACDE,OAAO,EAAE;IACPC,cAAcA,CAAA,EAAG;MACf,MAAMC,MAAK,GAAIC,QAAQ,CAACC,cAAc,CAAC,iBAAiB,CAAC;MACzD,MAAMC,UAAS,GAAIjB,QAAQ,CAACkB,MAAM,CAACJ,MAAM,EAAE;QAAEK,MAAM,EAAE;MAAK,CAAC,CAAC;MAC5DF,UAAU,CAAC;QACTG,aAAa,EAAE,GAAG;QAClBC,MAAM,EAAE,EAAE;QACVC,MAAM,EAAE;UAAEC,CAAC,EAAE;QAAI;MACnB,CAAC,CAAC;IACJ;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACA,MAAM,CAAC,iBAAiB,CAAC,CAACC,IAAI,CAAEC,MAAM,IAAK;MACzC,IAAI,CAACvB,kBAAiB,GAAIuB,MAAM,CAACC,kBAAkB;MACnD,IAAI,CAACd,cAAc,CAAC,CAAC;IACvB,CAAC,CAAC;IACF,MAAM,CAAC,iBAAiB,CAAC,CAACY,IAAI,CAAEC,MAAM,IAAK;MACzC,IAAI,CAACtB,qBAAoB,GAAIsB,MAAM,CAACE,kBAAkB;IACxD,CAAC,CAAC;EACJ;AACF,CAAC"},"metadata":{},"sourc
{"ast":null,"code":"import confetti from 'canvas-confetti';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      approvedCandidates: [],\n      notApprovedCandidates: [],\n      searchQuery: '',\n      activeTab: 'selected',\n      // Initialize the activeTab to 'selected'\n      sortOrder: 'desc',\n      currentSort: 'name',\n      // default sort property\n      currentSortDir: 'asc' // default sort direction\n    };\n  },\n\n  computed: {\n    sortedApprovedCandidates() {\n      return this.sortCandidates(this.approvedCandidates);\n    },\n    sortedRejectedCandidates() {\n      return this.sortCandidates(this.notApprovedCandidates);\n    },\n    filteredApprovedCandidates() {\n      return this.approvedCandidates.filter(candidate => candidate.name.toLowerCase().includes(this.searchQuery.toLowerCase()));\n    },\n    filteredRejectedCandidates() {\n      return this.notApprovedCandidates.filter(candidate => candidate.name.toLowerCase().includes(this.searchQuery.toLowerCase()));\n    }\n  },\n  methods: {\n    sortCandidates(candidates) {\n      return candidates.filter(candidate => candidate.name.toLowerCase().includes(this.searchQuery.toLowerCase())).sort((a, b) => {\n        let modifier = this.currentSortDir === 'asc' ? 1 : -1;\n        let aValue = this.getSortValue(a, this.currentSort);\n        let bValue = this.getSortValue(b, this.currentSort);\n        if (aValue < bValue) return -1 * modifier;\n        if (aValue > bValue) return 1 * modifier;\n        return 0;\n      });\n    },\n    getSortValue(candidate, sortKey) {\n      // Handle nested properties and different data types\n      if (sortKey in candidate.scoreBreakdown) {\n        return candidate.scoreBreakdown[sortKey];\n      } else if (sortKey === 'name') {\n        // Case insensitive sorting for names\n        return candidate[sortKey].toLowerCase();\n      } else {\n        return candidate[sortKey];\n      }\n    },\n    sort(s) {\n      if (s === this.currentSort) {\n        this.currentSortDir = this.currentSortDir === 'asc' ? 'desc' : 'asc';\n      } else {\n        this.currentSort = s;\n        this.currentSortDir = 'asc';\n      }\n      this.sortCandidates(this.approvedCandidates);\n      this.sortCandidates(this.notApprovedCandidates);\n      this.approvedCandidates = [...this.approvedCandidates];\n      this.notApprovedCandidates = [...this.notApprovedCandidates];\n    },\n    launchConfetti() {\n      const canvas = document.getElementById('confetti-canvas');\n      const myConfetti = confetti.create(canvas, {\n        resize: true\n      });\n      myConfetti({\n        particleCount: 150,\n        spread: 70,\n        origin: {\n          y: 0.6\n        }\n      });\n    }\n  },\n  mounted() {\n    // Fetch the candidate data from the JSON files when the component is created\n    import('./selected.json').then(module => {\n      this.approvedCandidates = module.selectedCandidates;\n      this.launchConfetti();\n    });\n    import('./rejected.json').then(module => {\n      this.notApprovedCandidates = module.rejectedCandidates;\n    });\n  }\n};","map":{"version":3,"names":["confetti","name","data","approvedCandidates","notApprovedCandidates","searchQuery","activeTab","sortOrder","currentSort","currentSortDir","computed","sortedApprovedCandidates","sortCandidates","sortedRejectedCandidates","filteredApprovedCandidates","filter","candidate","toLowerCase","includes","filteredRejectedCandidates","methods","candidates","sort","a","b","modifier","aValue","getSortValue","bValue","sortKey","scoreBreakdown","s","launchConfetti","canvas","document","getElementById","myConfetti","create","resize","particleCount","spread","origin","y","mounted","then","module","selectedCandidates","rejectedCandidates"],"sources":["/Users/keyurmaru/Binary_Tree/Binary_Tree/nursing-college-app/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"app\">\n    <div class=\"container\">\n      <!-- Tab buttons -->\n      <div class=\"tab-buttons\">\n        <button\n          :class=\"{'active': activeTab === 'selected'}\"\n          @click=\"activeTab = 'selected'\"\n        >Selected Candidates</button>\n        <button\n          :class=\"{'active': activeTab === 'rejected'}\"\n          @click=\"activeTab = 'rejected'\"\n        >Rejected Candidates</button>\n      </div>\n      <!-- Search bar -->\n      <div class=\"search-bar\">\n        <input\n          v-model=\"searchQuery\"\n          type=\"text\"\n          placeholder=\"Search by name...\"\n        />\n      </div>\n      <!-- Candidates list based on active tab -->\n      <div class=\"candidates\" v-if=\"activeTab === 'selected'\">\n        <div v-for=\"candidate in sortedApprovedCandidates\" :key=\"candidate.name\" class=\"p-4 bg-white shadow rounded mb-3\">\n        <table class=\"min-w-full table-auto border-collapse border border-gray-200 mt-4\">\n          <thead class=\"bg-gray-100\">\n            <tr>\n              <th @click=\"sort('name')\">Student Name <span v-if=\"currentSort === 'name'\">{{ currentSortDir === 'asc' ? '↑' : '↓' }}</span></th>\n              <th @click=\"sort('teasScore')\">TEAS Score <span v-if=\"currentSort === 'teasScore'\">{{ currentSortDir === 'asc' ? '↑' : '↓' }}</span></th>\n              <th @click=\"sort('nonScienceGpa')\">Non-Science GPA <span v-if=\"currentSort === 'nonScienceGpa'\">{{ currentSortDir === 'asc' ? '↑' : '↓' }}</span></th>\n              <th @click=\"sort('scienceGpa')\">Science GPA <span v-if=\"currentSort === 'scienceGpa'\">{{ currentSortDir === 'asc' ? '↑' : '↓' }}</span></th>\n              <th @click=\"sort('hasAllPrereqs')\">Has all Pre-Reqs? <span v-if=\"currentSort === 'hasAllPrereqs'\">{{ currentSortDir === 'asc' ? '↑' : '↓' }}</span></th>\n              <th @click=\"sort('hasAaAs')\">Has AA/AS? <span v-if=\"currentSort === 'hasAaAs'\">{{ currentSortDir === 'asc' ? '↑' : '↓' }}</span></th>\n              <th @click=\"sort('totalScore')\">Total Score <span v-if=\"currentSort === 'totalScore'\">{{ currentSortDir === 'asc' ? '↑' : '↓' }}</span></th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr class=\"hover:bg-gray-50\">\n              <td class=\"border border-gray-300 p-2\">{{ candidate.name }}</td>\n              <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['TEAS Score >= 62?'] }}</td>\n              <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Non-Science GPA >= 2.5?'] }}</td>\n              <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Science GPA >= 3.0?'] }}</td>\n              <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Has all Pre-Reqs?'] }}</td>\n              <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Has AA/AS?'] }}</td>\n              <td class=\"border border-gray-300 p-2\">{{ candidate.totalScore }}</td>\n            </tr>\n          </tbody>\n        </table>\n        </div>\n  </div>\n  <div v-else>\n  <div v-for=\"candidate in sortedRejectedCandidates\" :key=\"candidate.name\" class=\"p-4 bg-white shadow rounded mb-3\">\n    <h3 class=\"text-xl font-bold\">{{ candidate.name }}</h3>\n      <p>Total Score: {{ candidate.totalScore }}</p>\n      <table class=\"min-w-full table-auto border-collapse border border-gray-200 mt-4\">\n        <thead class=\"bg-gray-100\">\n          <tr>\n            <th @click=\"sort('name')\">Student Name <span v-if=\"currentSort === 'name'\">{{ currentSortDir === 'asc' ? '↑' : '↓' }}</span></th>\n            <th @click=\"sort('teasScore')\">TEAS Score <span v-if=\"currentSort === 'teasScore'\">{{ currentSortDir === 'asc' ? '↑' : '↓' }}</span></th>\n            <th @click=\"sort('nonScienceGpa')\">Non-Science GPA <span v-if=\"currentSort === 'nonScienceGpa'\">{{ currentSortDir === 'asc' ? '↑' : '↓' }}</span></th>\n            <th @click=\"sort('scienceGpa')\">Science GPA <span v-if=\"currentSort === 'scienceGpa'\">{{ currentSortDir === 'asc' ? '↑' : '↓' }}</span></th>\n            <th @click=\"sort('hasAllPrereqs')\">Has all Pre-Reqs? <span v-if=\"currentSort === 'hasAllPrereqs'\">{{ currentSortDir === 'asc' ? '↑' : '↓' }}</span></th>\n            <th @click=\"sort('hasAaAs')\">Has AA/AS? <span v-if=\"currentSort === 'hasAaAs'\">{{ currentSortDir === 'asc' ? '↑' : '↓' }}</span></th>\n            <th @click=\"sort('totalScore')\">Total Score <span v-if=\"currentSort === 'totalScore'\">{{ currentSortDir === 'asc' ? '↑' : '↓' }}</span></th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr class=\"hover:bg-gray-50\">\n            <td class=\"border border-gray-300 p-2\">{{ candidate.name }}</td>\n            <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['TEAS Score >= 62?'] }}</td>\n            <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Non-Science GPA >= 2.5?'] }}</td>\n            <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Science GPA >= 3.0?'] }}</td>\n            <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Has all Pre-Reqs?'] }}</td>\n            <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Has AA/AS?'] }}</td>\n            <td class=\"border border-gray-300 p-2\">{{ candidate.totalScore }}</td>\n          </tr>\n        </tbody>\n      </table>\n  </div>\n  </div>\n  </div>\n  </div>\n  <canvas id=\"confetti-canvas\"></canvas>\n\n</template>\n\n<script>\nimport confetti from 'canvas-confetti';\n\nexport default {\n  name: 'App',\n  data() {\n    return {\n      approvedCandidates: [],\n      notApprovedCandidates: [],\n      searchQuery: '',\n      activeTab: 'selected', // Initialize the activeTab to 'selected'\n      sortOrder: 'desc',\n      currentSort: 'name', // default sort property\n      currentSortDir: 'asc', // default sort direction\n    };\n  },\n  computed: {\n    sortedApprovedCandidates() {\n      return this.sortCandidates(this.approvedCandidates);\n    },\n    sortedRejectedCandidates() {\n      return this.sortCandidates(this.notApprovedCandidates);\n    },\n  filteredApprovedCandidates() {\n    return this.approvedCandidates.filter(candidate =>\n      candidate.name.toLowerCase().includes(this.searchQuery.toLowerCase())\n    );\n  },   \n  filteredRejectedCandidates() {\n    return this.notApprovedCandidates.filter(candidate =>\n      candidate.name.toLowerCase().includes(this.searchQuery.toLowerCase())\n    );\n  },\n  },\n  methods: {\n    sortCandidates(candidates) {\n    return candidates\n      .filter(candidate =>\n        candidate.name.toLowerCase().includes(this.searchQuery.toLowerCase())\n      )\n      .sort((a, b) => {\n        let modifier = this.currentSortDir === 'asc' ? 1 : -1;\n        let aValue = this.getSortValue(a, this.currentSort);\n        let bValue = this.getSortValue(b, this.currentSort);\n        if (aValue < bValue) return -1 * modifier;\n        if (aValue > bValue) return 1 * modifier;\n        return 0;\n      });\n  },\n  getSortValue(candidate, sortKey) {\n    // Handle nested properties and different data types\n    if (sortKey in candidate.scoreBreakdown) {\n      return candidate.scoreBreakdown[sortKey];\n    } else if (sortKey === 'name') {\n      // Case insensitive sorting for names\n      return candidate[sortKey].toLowerCase();\n    } else {\n      return candidate[sortKey];\n    }\n  },\n  sort(s) {\n    if (s === this.currentSort) {\n      this.currentSortDir = this.currentSortDir === 'asc' ? 'desc' : 'asc';\n    } else {\n      this.currentSort = s;\n      this.currentSortDir = 'asc';\n    }\n    this.sortCandidates(this.approvedCandidates);\n    this.sortCandidates(this.notApprovedCandidates);\n    this.approvedCandidates = [...this.approvedCandidates];\n    this.notApprovedCandidates = [...this.notApprovedCandidates];\n  },\n    launchConfetti() {\n      const canvas = document.getElementById('confetti-canvas');\n      const myConfetti = confetti.create(canvas, { resize: true });\n      myConfetti({\n        particleCount: 150,\n        spread: 70,\n        origin: { y: 0.6 },\n      });\n    },\n  },\n  mounted() {\n    // Fetch the candidate data from the JSON files when the component is created\n    import('./selected.json').then((module) => {\n      this.approvedCandidates = module.selectedCandidates;\n      this.launchConfetti();\n    });\n    import('./rejected.json').then((module) => {\n      this.notApprovedCandidates = module.rejectedCandidates;\n    });\n  },\n};\n</script>\n\n<style>\n#confetti-canvas {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  pointer-events: none;\n  z-index: 9999;\n  }\n  \n.app {\n  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen,\n    Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;\n  color: #333;\n  background-color: #f4f4f9;\n  transition: background-color 0.3s, color 0.3s;\n}\n\n.container {\n  max-width: 1200px;\n  margin: 0 auto;\n  padding: 2rem;\n}\n\n.tab-buttons button {\n  background-color: transparent;\n  border: none;\n  padding: 1rem 2rem;\n  margin-right: 1rem;\n  font-size: 1rem;\n  border-radius: 50px;\n  transition: background-color 0.3s;\n}\n\n.tab-buttons button.active {\n  background-color: #007aff;\n  color: white;\n}\n\n.search-bar {\n  margin: 2rem 0;\n}\n\n.search-bar input {\n  width: 100%;\n  padding: 1rem;\n  border: none;\n  border-radius: 50px;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n}\n\n.candidates {\n  background-color: white;\n  box-shadow: 0 6px 8px rgba(0, 0, 0, 0.1);\n  border-radius: 20px;\n  overflow: hidden;\n  margin-bottom: 2rem;\n}\n\ntable {\n  width: 100%;\n  border-collapse: separate;\n  border-spacing: 0;\n  margin-top: 1rem;\n}\n\nth, td {\n  text-align: left;\n  padding: 1rem;\n  border-bottom: 1px solid #eaeaea;\n}\n\nth:first-child, td:first-child {\n  border-top-left-radius: 20px;\n}\n\nth:last-child, td:last-child {\n  border-top-right-radius: 20px;\n}\n\nthead {\n  background-color: #fafafa;\n}\n\ntbody tr:hover {\n  background-color: #f0f0f0;\n}\n\n/* Dark Mode */\n.dark-mode {\n  background-color: #1c1c1e;\n  color: #f4f4f9;\n}\n\n.dark-mode .candidates {\n  background-color: #2c2c2e;\n}\n\n.dark-mode .search-bar input {\n  background-color: #3a3a3c;\n  color: #f4f4f9;\n}\n\n.dark-mode table {\n  border: 1px solid #444;\n}\n\n.dark-mode th, .dark-mode td {\n  border-bottom: 1px solid #444;\n}\n\n.dark-mode thead {\n  background-color: #3a3a3c;\n}\n\n.dark-mode tbody tr:hover {\n  background-color: #48484a;\n} \n</style>\n"],"mappings":"AAwFA,OAAOA,QAAO,MAAO,iBAAiB;AAEtC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,kBAAkB,EAAE,EAAE;MACtBC,qBAAqB,EAAE,EAAE;MACzBC,WAAW,EAAE,EAAE;MACfC,SAAS,EAAE,UAAU;MAAE;MACvBC,SAAS,EAAE,MAAM;MACjBC,WAAW,EAAE,MAAM;MAAE;MACrBC,cAAc,EAAE,KAAK,CAAE;IACzB,CAAC;EACH,CAAC;;EACDC,QAAQ,EAAE;IACRC,wBAAwBA,CAAA,EAAG;MACzB,OAAO,IAAI,CAACC,cAAc,CAAC,IAAI,CAACT,kBAAkB,CAAC;IACrD,CAAC;IACDU,wBAAwBA,CAAA,EAAG;MACzB,OAAO,IAAI,CAACD,cAAc,CAAC,IAAI,CAACR,qBAAqB,CAAC;IACxD,CAAC;IACHU,0BAA0BA,CAAA,EAAG;MAC3B,OAAO,IAAI,CAACX,kBAAkB,CAACY,MAAM,CAACC,SAAQ,IAC5CA,SAAS,CAACf,IAAI,CAACgB,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACb,WAAW,CAACY,WAAW,CAAC,CAAC,CACtE,CAAC;IACH,CAAC;IACDE,0BAA0BA,CAAA,EAAG;MAC3B,OAAO,IAAI,CAACf,qBAAqB,CAACW,MAAM,CAACC,SAAQ,IAC/CA,SAAS,CAACf,IAAI,CAACgB,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACb,WAAW,CAACY,WAAW,CAAC,CAAC,CACtE,CAAC;IACH;EACA,CAAC;EACDG,OAAO,EAAE;IACPR,cAAcA,CAACS,UAAU,EAAE;MAC3B,OAAOA,UAAS,CACbN,MAAM,CAACC,SAAQ,IACdA,SAAS,CAACf,IAAI,CAACgB,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACb,WAAW,CAACY,WAAW,CAAC,CAAC,CACtE,EACCK,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;QACd,IAAIC,QAAO,GAAI,IAAI,CAAChB,cAAa,KAAM,KAAI,GAAI,IAAI,CAAC,CAAC;QACrD,IAAIiB,MAAK,GAAI,IAAI,CAACC,YAAY,CAACJ,CAAC,EAAE,IAAI,CAACf,WAAW,CAAC;QACnD,IAAIoB,MAAK,GAAI,IAAI,CAACD,YAAY,CAACH,CAAC,EAAE,IAAI,CAAChB,WAAW,CAAC;QACnD,IAAIkB,MAAK,GAAIE,MAAM,EAAE,OAAO,CAAC,IAAIH,QAAQ;QACzC,IAAIC,MAAK,GAAIE,MAAM,E
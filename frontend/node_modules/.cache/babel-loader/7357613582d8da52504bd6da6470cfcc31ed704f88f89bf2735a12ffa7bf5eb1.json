{"ast":null,"code":"import confetti from 'canvas-confetti';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      approvedCandidates: [],\n      notApprovedCandidates: [],\n      searchQuery: '',\n      activeTab: 'selected' // Initialize the activeTab to 'selected'\n    };\n  },\n\n  computed: {\n    filteredApprovedCandidates() {\n      return this.approvedCandidates.filter(candidate => candidate.name.toLowerCase().includes(this.searchQuery.toLowerCase()));\n    },\n    filteredRejectedCandidates() {\n      return this.notApprovedCandidates.filter(candidate => candidate.name.toLowerCase().includes(this.searchQuery.toLowerCase()));\n    },\n    // add total score to each candidate from the json file \n    // sample\n    // {\"selectedCandidates\": [\n    // {\n    //     \"scoreBreakdown\": {\n    //         \"TEAS Score >= 62?\": 20,\n    //         \"Non-Science GPA >= 2.5?\": 25,\n    //         \"Science GPA >= 3.0?\": 25,\n    //         \"Has all Pre-Reqs?\": 20,\n    //         \"Has AA/AS?\": 10\n    //     },\n    //     \"name\": \"William Jones\",\n    //     \"totalScore\": 100\n    // },\n    addTotalScore() {\n      this.approvedCandidates.forEach(candidate => {\n        let totalScore = 0;\n        for (let key in candidate.scoreBreakdown) {\n          totalScore += candidate.scoreBreakdown[key];\n        }\n        candidate.totalScore = totalScore;\n      });\n      this.notApprovedCandidates.forEach(candidate => {\n        let totalScore = 0;\n        for (let key in candidate.scoreBreakdown) {\n          totalScore += candidate.scoreBreakdown[key];\n        }\n        candidate.totalScore = totalScore;\n      });\n    }\n  },\n  methods: {\n    launchConfetti() {\n      const canvas = document.getElementById('confetti-canvas');\n      const myConfetti = confetti.create(canvas, {\n        resize: true\n      });\n      myConfetti({\n        particleCount: 150,\n        spread: 70,\n        origin: {\n          y: 0.6\n        }\n      });\n    }\n  },\n  mounted() {\n    // Fetch the candidate data from the JSON files when the component is created\n    import('./selected.json').then(module => {\n      this.approvedCandidates = module.selectedCandidates;\n      // add total score to each candidate\n      this.addTotalScore;\n      this.launchConfetti();\n    });\n    import('./rejected.json').then(module => {\n      this.notApprovedCandidates = module.rejectedCandidates;\n      this.addTotalScore;\n    });\n  }\n};","map":{"version":3,"names":["confetti","name","data","approvedCandidates","notApprovedCandidates","searchQuery","activeTab","computed","filteredApprovedCandidates","filter","candidate","toLowerCase","includes","filteredRejectedCandidates","addTotalScore","forEach","totalScore","key","scoreBreakdown","methods","launchConfetti","canvas","document","getElementById","myConfetti","create","resize","particleCount","spread","origin","y","mounted","then","module","selectedCandidates","rejectedCandidates"],"sources":["/Users/keyurmaru/Binary_Tree/Binary_Tree/nursing-college-app/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"app\">\n    <div class=\"container\">\n      <!-- Tab buttons -->\n      <div class=\"tab-buttons\">\n        <button\n          :class=\"{'active': activeTab === 'selected'}\"\n          @click=\"activeTab = 'selected'\"\n        >Selected Candidates</button>\n        <button\n          :class=\"{'active': activeTab === 'rejected'}\"\n          @click=\"activeTab = 'rejected'\"\n        >Rejected Candidates</button>\n      </div>\n      <!-- Search bar -->\n      <div class=\"search-bar\">\n        <input\n          v-model=\"searchQuery\"\n          type=\"text\"\n          placeholder=\"Search by name...\"\n        />\n      </div>\n      <!-- Candidates list based on active tab -->\n      <div class=\"candidates\" v-if=\"activeTab === 'selected'\">\n        <div v-for=\"candidate in filteredApprovedCandidates\" :key=\"candidate.name\" class=\"p-4 bg-white shadow rounded mb-3\">\n        <table class=\"min-w-full table-auto border-collapse border border-gray-200 mt-4\">\n          <thead class=\"bg-gray-100\">\n            <tr>\n              <th class=\"border border-gray-300 p-2\">Student Name</th>\n              <th class=\"border border-gray-300 p-2\">TEAS Score >= 62?</th>\n              <th class=\"border border-gray-300 p-2\">Non-Science GPA >= 2.5?</th>\n              <th class=\"border border-gray-300 p-2\">Science GPA >= 3.0?</th>\n              <th class=\"border border-gray-300 p-2\">Has all Pre-Reqs?</th>\n              <th class=\"border border-gray-300 p-2\">Has AA/AS?</th>\n              <th class=\"border border-gray-300 p-2\">Total Score</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr class=\"hover:bg-gray-50\">\n              <td class=\"border border-gray-300 p-2\">{{ candidate.name }}</td>\n              <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['TEAS Score >= 62?'] }}</td>\n              <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Non-Science GPA >= 2.5?'] }}</td>\n              <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Science GPA >= 3.0?'] }}</td>\n              <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Has all Pre-Reqs?'] }}</td>\n              <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Has AA/AS?'] }}</td>\n            </tr>\n          </tbody>\n        </table>\n        </div>\n  </div>\n  <div v-else>\n    <div v-for=\"candidate in filteredRejectedCandidates\" :key=\"candidate.name\" class=\"p-4 bg-white shadow rounded mb-3\">\n      <h3 class=\"text-xl font-bold\">{{ candidate.name }}</h3>\n      <p>Total Score: {{ candidate.totalScore }}</p>\n      <table class=\"min-w-full table-auto border-collapse border border-gray-200 mt-4\">\n        <thead class=\"bg-gray-100\">\n          <tr>\n            <th class=\"border border-gray-300 p-2\">Student Name</th>\n            <th class=\"border border-gray-300 p-2\">TEAS Score >= 62?</th>\n            <th class=\"border border-gray-300 p-2\">Non-Science GPA >= 2.5?</th>\n            <th class=\"border border-gray-300 p-2\">Science GPA >= 3.0?</th>\n            <th class=\"border border-gray-300 p-2\">Has all Pre-Reqs?</th>\n            <th class=\"border border-gray-300 p-2\">Has AA/AS?</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr class=\"hover:bg-gray-50\">\n            <td class=\"border border-gray-300 p-2\">{{ candidate.name }}</td>\n            <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['TEAS Score >= 62?'] }}</td>\n            <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Non-Science GPA >= 2.5?'] }}</td>\n            <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Science GPA >= 3.0?'] }}</td>\n            <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Has all Pre-Reqs?'] }}</td>\n            <td class=\"border border-gray-300 p-2\">{{ candidate.scoreBreakdown['Has AA/AS?'] }}</td>\n          </tr>\n        </tbody>\n      </table>\n  </div>\n  </div>\n  </div>\n  </div>\n  <canvas id=\"confetti-canvas\"></canvas>\n\n</template>\n\n<script>\nimport confetti from 'canvas-confetti';\n\nexport default {\n  name: 'App',\n  data() {\n    return {\n      approvedCandidates: [],\n      notApprovedCandidates: [],\n      searchQuery: '',\n      activeTab: 'selected', // Initialize the activeTab to 'selected'\n    };\n  },\n  computed: {\n    filteredApprovedCandidates() {\n      return this.approvedCandidates.filter(candidate =>\n        candidate.name.toLowerCase().includes(this.searchQuery.toLowerCase())\n      );\n    },\n    filteredRejectedCandidates() {\n      return this.notApprovedCandidates.filter(candidate =>\n        candidate.name.toLowerCase().includes(this.searchQuery.toLowerCase())\n      );\n    },\n    // add total score to each candidate from the json file \n    // sample\n    // {\"selectedCandidates\": [\n    // {\n    //     \"scoreBreakdown\": {\n    //         \"TEAS Score >= 62?\": 20,\n    //         \"Non-Science GPA >= 2.5?\": 25,\n    //         \"Science GPA >= 3.0?\": 25,\n    //         \"Has all Pre-Reqs?\": 20,\n    //         \"Has AA/AS?\": 10\n    //     },\n    //     \"name\": \"William Jones\",\n    //     \"totalScore\": 100\n    // },\n    addTotalScore() {\n      this.approvedCandidates.forEach(candidate => {\n        let totalScore = 0;\n        for (let key in candidate.scoreBreakdown) {\n          totalScore += candidate.scoreBreakdown[key];\n        }\n        candidate.totalScore = totalScore;\n      });\n      this.notApprovedCandidates.forEach(candidate => {\n        let totalScore = 0;\n        for (let key in candidate.scoreBreakdown) {\n          totalScore += candidate.scoreBreakdown[key];\n        }\n        candidate.totalScore = totalScore;\n      });\n    },\n  },\n  methods: {\n    launchConfetti() {\n      const canvas = document.getElementById('confetti-canvas');\n      const myConfetti = confetti.create(canvas, { resize: true });\n      myConfetti({\n        particleCount: 150,\n        spread: 70,\n        origin: { y: 0.6 },\n      });\n    },\n  },\n  mounted() {\n    // Fetch the candidate data from the JSON files when the component is created\n    import('./selected.json').then((module) => {\n      this.approvedCandidates = module.selectedCandidates;\n      // add total score to each candidate\n      this.addTotalScore;\n      this.launchConfetti();\n    });\n    import('./rejected.json').then((module) => {\n      this.notApprovedCandidates = module.rejectedCandidates;\n      this.addTotalScore;\n    });\n  },\n};\n</script>\n\n<style>\n#confetti-canvas {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  pointer-events: none;\n  z-index: 9999;\n  }\n  \n.app {\n  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen,\n    Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;\n  color: #333;\n  background-color: #f4f4f9;\n  transition: background-color 0.3s, color 0.3s;\n}\n\n.container {\n  max-width: 1200px;\n  margin: 0 auto;\n  padding: 2rem;\n}\n\n.tab-buttons button {\n  background-color: transparent;\n  border: none;\n  padding: 1rem 2rem;\n  margin-right: 1rem;\n  font-size: 1rem;\n  border-radius: 50px;\n  transition: background-color 0.3s;\n}\n\n.tab-buttons button.active {\n  background-color: #007aff;\n  color: white;\n}\n\n.search-bar {\n  margin: 2rem 0;\n}\n\n.search-bar input {\n  width: 100%;\n  padding: 1rem;\n  border: none;\n  border-radius: 50px;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n}\n\n.candidates {\n  background-color: white;\n  box-shadow: 0 6px 8px rgba(0, 0, 0, 0.1);\n  border-radius: 20px;\n  overflow: hidden;\n  margin-bottom: 2rem;\n}\n\ntable {\n  width: 100%;\n  border-collapse: separate;\n  border-spacing: 0;\n  margin-top: 1rem;\n}\n\nth, td {\n  text-align: left;\n  padding: 1rem;\n  border-bottom: 1px solid #eaeaea;\n}\n\nth:first-child, td:first-child {\n  border-top-left-radius: 20px;\n}\n\nth:last-child, td:last-child {\n  border-top-right-radius: 20px;\n}\n\nthead {\n  background-color: #fafafa;\n}\n\ntbody tr:hover {\n  background-color: #f0f0f0;\n}\n\n/* Dark Mode */\n.dark-mode {\n  background-color: #1c1c1e;\n  color: #f4f4f9;\n}\n\n.dark-mode .candidates {\n  background-color: #2c2c2e;\n}\n\n.dark-mode .search-bar input {\n  background-color: #3a3a3c;\n  color: #f4f4f9;\n}\n\n.dark-mode table {\n  border: 1px solid #444;\n}\n\n.dark-mode th, .dark-mode td {\n  border-bottom: 1px solid #444;\n}\n\n.dark-mode thead {\n  background-color: #3a3a3c;\n}\n\n.dark-mode tbody tr:hover {\n  background-color: #48484a;\n} \n</style>\n"],"mappings":"AAqFA,OAAOA,QAAO,MAAO,iBAAiB;AAEtC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,kBAAkB,EAAE,EAAE;MACtBC,qBAAqB,EAAE,EAAE
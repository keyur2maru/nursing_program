{"ast":null,"code":"import confetti from 'canvas-confetti';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      approvedCandidates: [],\n      notApprovedCandidates: [],\n      searchQuery: '',\n      activeTab: 'selected',\n      currentSort: 'name',\n      currentSortDir: 'asc',\n      isDarkMode: false\n    };\n  },\n  computed: {\n    sortedApprovedCandidates() {\n      return this.sortCandidates(this.filteredApprovedCandidates);\n    },\n    sortedRejectedCandidates() {\n      return this.sortCandidates(this.filteredRejectedCandidates);\n    },\n    filteredApprovedCandidates() {\n      return this.approvedCandidates.filter(candidate => candidate.name.toLowerCase().includes(this.searchQuery.toLowerCase()));\n    },\n    filteredRejectedCandidates() {\n      return this.notApprovedCandidates.filter(candidate => candidate.name.toLowerCase().includes(this.searchQuery.toLowerCase()));\n    }\n  },\n  methods: {\n    toggleDarkMode() {\n      this.isDarkMode = !this.isDarkMode;\n    },\n    getAriaSort(column) {\n      if (this.currentSort === column) {\n        return this.currentSortDir === 'asc' ? 'ascending' : 'descending';\n      }\n      return 'none';\n    },\n    getColumnFriendlyName(column) {\n      const friendlyNames = {\n        name: 'Student Name',\n        scienceGPAGreaterThan3_0: 'Science GPA',\n        teasScoreGreaterThan62: 'TEAS Score',\n        nonScienceGPAGreaterThan2_5: 'Non-Science GPA',\n        hasAllPreReqs: 'Pre-Requisite Status',\n        hasAaAs: 'Degree Status',\n        totalScore: 'Total Score'\n      };\n      return friendlyNames[column] || column;\n    },\n    sortCandidates(candidates) {\n      // Sorting without mutating the original array\n      return [...candidates].sort((a, b) => {\n        let modifier = this.currentSortDir === 'asc' ? 1 : -1;\n        let aValue = this.getSortValue(a, this.currentSort);\n        let bValue = this.getSortValue(b, this.currentSort);\n        if (aValue < bValue) return -1 * modifier;\n        if (aValue > bValue) return 1 * modifier;\n        return 0;\n      });\n    },\n    getSortValue(candidate, sortKey) {\n      if (sortKey in candidate.scoreBreakdown) {\n        let value = candidate.scoreBreakdown[sortKey];\n        console.log(\"before sort: \" + value);\n        // Check if the value is a number and convert if necessary\n        if (!isNaN(parseFloat(value)) && isFinite(value)) {\n          return Number(value);\n        }\n        return value;\n      } else if (sortKey === 'name') {\n        // Case insensitive sorting for names\n        return candidate[sortKey].toLowerCase();\n      } else {\n        let value = candidate[sortKey];\n        // Convert to number if possible for proper comparison\n        if (!isNaN(parseFloat(value)) && isFinite(value)) {\n          return Number(value);\n        }\n        console.log(\"after sort: \" + value);\n        return value;\n      }\n    },\n    sort(s) {\n      if (s === this.currentSort) {\n        this.currentSortDir = this.currentSortDir === 'asc' ? 'desc' : 'asc';\n      } else {\n        this.currentSort = s;\n        this.currentSortDir = 'asc';\n      }\n    },\n    launchConfetti() {\n      const canvas = document.getElementById('confetti-canvas');\n      const myConfetti = confetti.create(canvas, {\n        resize: true\n      });\n      myConfetti({\n        particleCount: 150,\n        spread: 70,\n        origin: {\n          y: 0.6\n        }\n      });\n    }\n  },\n  mounted() {\n    // Fetch the candidate data from the JSON files when the component is created\n    import('./selected.json').then(module => {\n      this.approvedCandidates = module.selectedCandidates;\n      this.launchConfetti();\n    });\n    import('./rejected.json').then(module => {\n      this.notApprovedCandidates = module.rejectedCandidates;\n    });\n  }\n};","map":{"version":3,"names":["confetti","name","data","approvedCandidates","notApprovedCandidates","searchQuery","activeTab","currentS